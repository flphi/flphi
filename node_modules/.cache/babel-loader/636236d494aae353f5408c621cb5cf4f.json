AA,KAAA,CAAA,WAAA,GAAA,IAAA;AACA,OANA;AAOA,MAAA,KAAA,CAAA,cAAA;AACA;AAhBA;AAjBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"SendMail\">\r\n    <h1>Send Mail to Users</h1>\r\n\r\n    <div class=\"container\">\r\n      <div class=\"alert alert-danger\" role=\"alert\" v-if=\"invalid\">\r\n        All fields are required\r\n      </div>\r\n      <div class=\"alert alert-danger\" role=\"alert\" v-if=\"serverError\">\r\n        Server error\r\n      </div>\r\n      <form>\r\n        <div class=\"row\">\r\n          <div class=\"col-md-12\">\r\n            <div class=\"form-group\">\r\n              <label for=\"title\">Title</label>\r\n              <input type=\"text\" class=\"form-control\" v-model=\"title\"/>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"col-md-12\">\r\n            <div class=\"form-group\">\r\n              <label for=\"content\">Content</label>\r\n              <textarea class=\"form-control\" id=\"exampleFormControlTextarea1\" rows=\"3\" v-model=\"content\"></textarea>\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"col-md-12\">\r\n            <input type=\"submit\" class=\"btn btn-primary\" v-on:click=\"onSendClick($event)\">\r\n          </div>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport RestClient from '../utils/rest-client';\r\nimport UserAuthentification from '../utils/user-authentification';\r\n\r\nexport default {\r\n  name: \"Sendmail\",\r\n  props: [],\r\n  data: function() {\r\n    return {\r\n      title: '',\r\n      content: '',\r\n      invalid: false,\r\n      serverError: false\r\n    };\r\n  },\r\n  created() {\r\n    if(!UserAuthentification.isCurrentUserSuperUser()){\r\n      this.$router.push({ path: '/login' });\r\n      return\r\n    }\r\n  },\r\n  methods: {\r\n    onSendClick: function(event) {\r\n      if (!this.title.length || !this.content.length) {\r\n        this.invalid = true;\r\n        return;\r\n      } else {\r\n        this.invalid = false\r\n      }\r\n      RestClient.post('/send/mail', { title: this.title, content: this.content})\r\n        .then((response) => {\r\n          this.serverError = false;\r\n          this.$router.push({ path: '/users' });\r\n        }).catch((error) => {\r\n          this.serverError = true;\r\n        });\r\n      event.preventDefault();\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n  .container {\r\n    text-align: left;\r\n  }\r\n</style>"],"sourceRoot":"src/views"}]}