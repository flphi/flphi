A,IADA,CACA,KAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,SAAA,KAAA,IAAA,CAAA,SAAA;AAAA,OAAA,CADA,EAEA,KAFA,CAEA,UAAA,GAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,OAFA;AAGA;AARA;AAnBA,CAAA","sourcesContent":["<template>\r\n  <div class=\"Register\">\r\n    <h1>Users</h1>\r\n    <div class=\"container\">\r\n      <div class=\"col-md-12\">\r\n          <router-link to=\"register\" tag=\"a\" class=\"btn btn-primary\" v-if=\"currentUser.super_user\">Add user</router-link>\r\n        </div>\r\n      <table class=\"table\">\r\n        <tbody>\r\n          <th>Email</th>\r\n          <th>Username</th>\r\n          <th>Superuser</th>\r\n          <th v-if=\"currentUser.super_user\">Delete</th>\r\n        </tbody>\r\n        <tbody>\r\n          <tr v-for=\"user in users\" v-bind:key=\"user.public_id\">\r\n            <td>{{ user.email }}</td>\r\n            <td>{{ user.name }}</td>\r\n            <td v-if=\"currentUser.super_user\">\r\n              <select @change=\"onSuperUserChange($event, user)\">\r\n                <option value=\"true\" :selected=\"user.super_user == true\">Yes</option>\r\n                <option value=\"false\" :selected=\"user.super_user == false\">No</option>\r\n              </select>\r\n            </td>\r\n            <td v-else>\r\n              {{  user.super_user  }}\r\n            </td>\r\n            <td v-if=\"currentUser.super_user\">\r\n              <button type=\"button\" class=\"btn btn-danger\" @click=\"delUser($event, user)\">X</button>\r\n            </td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script type=\"text/javascript\">\r\nimport RestClient from '../utils/rest-client';\r\n//import UserAuthentification from './Login';\r\nimport UserAuthentification from '../utils/user-authentification';\r\n\r\nexport default {\r\n  name: \"Users\",\r\n  props: [],\r\n  data: function() {\r\n    return {\r\n      users: [],\r\n      currentUser: null,\r\n    };\r\n  },\r\n  created() {\r\n    if(!UserAuthentification.isAuthenticated()){\r\n      this.$router.push({ path: '/login' });\r\n      return\r\n    }\r\n    RestClient.get(\"user\")\r\n      .then(res => this.users = res.data.users)\r\n      .catch(err => console.log(err));\r\n    this.currentUser = UserAuthentification.getCurrentUser();\r\n  },\r\n  methods: {\r\n      onSuperUserChange: function(event, user) {\r\n        RestClient.put(`user/${user.public_id}`, {super_user: JSON.parse(event.target.value)});\r\n    },\r\n      delUser: function(event, user) {\r\n        RestClient.delete(`user/${user.public_id}`)\r\n        .then(this.users = this.users.filter((u) => u.public_id !== user.public_id))\r\n        .catch(err => console.log(err));\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  text-align: left;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}